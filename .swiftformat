--exclude **/Pods,vendor,**/*generated.swift,**/Autogenerated,node_modules,**/Resources/GraphQL/Models.swift

# Options
--allman false
--elseposition nextline
--empty void
--ifdef no-indent
--indent 4
--nospaceoperators
--semicolons never
--stripunusedargs closure-only
--swiftversion 5.3
--trimwhitespace always
--xcodeindentation enabled

# Rules
--rules blankLinesBetweenScopes,braces,consecutiveBlankLines,consecutiveSpaces,duplicateImports,emptyBraces,indent,linebreakAtEndOfFile,spaceAroundOperators,redundantBackticks,redundantBreak,redundantGet,redundantInit,redundantLet,redundantLetError,redundantNilInit,redundantObjc,redundantParens,redundantPattern,redundantVoidReturnType,spaceAroundBraces,spaceAroundBrackets,spaceAroundGenerics,spaceAroundParens,spaceInsideBraces,spaceInsideBrackets,spaceInsideGenerics,spaceInsideParens,trailingCommas,trailingSpace,typeSugar,unusedArguments,void,yodaConditions,modifierOrder,elseOnSameLine,semicolons
